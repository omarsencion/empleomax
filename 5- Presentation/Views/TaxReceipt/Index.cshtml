@model IEnumerable<EmpleosWebMax.Domain.Dtos.TaxReceiptDto>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="AddTaxReceipt">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Code)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Prefix)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SequenceFrom)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SequenceTo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SequenceActual)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsCompany)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsCanditate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsInternationalCompany)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Code)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Prefix)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SequenceFrom)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SequenceTo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SequenceActual)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsCompany)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsCanditate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IsInternationalCompany)
            </td>
            <td>
                @Html.ActionLink("Editar", "EditTaxReceipt", new {  id=item.Id  }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
}
    </tbody>
</table>
