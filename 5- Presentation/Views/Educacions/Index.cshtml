@model IEnumerable<EmpleosWebMax.Domain.Entity.Educacion>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_LayoutBlank.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.IdUser)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.tipoestudio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Institucion)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.InstitucionLugar)
